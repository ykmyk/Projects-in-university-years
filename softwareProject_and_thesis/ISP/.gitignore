# create .gitignore
cat > .gitignore << 'EOF'
# macOS
.DS_Store

# Python
__pycache__/
*.py[cod]
*.pyo

# Django
*.sqlite3
*.log
staticfiles/
media/

# VS Code
.vscode/
# Envs
.venv/
ENV/
venv/
EOF

# untrack previously committed files/folders
git rm -r --cached .DS_Store .vscode __pycache__ */__pycache__ || true
git add .gitignore
git commit -m "Add .gitignore; stop tracking generated files"
git push

